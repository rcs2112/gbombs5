"UnlitGeneric"
{
	"$baseTexture" "hud/radiation"
	"$translucent"		"1"
	"$alpha" 1
	"$vertexalpha" "1"
	"$ignorez"     "1"
	"$no_fullbright" "1"
	"Proxies"
    {
      "AnimatedTexture"
      {
         "animatedtexturevar" "$baseTexture"
         "animatedtextureframenumvar" "$frame"
         "animatedtextureframerate" "20"
      }
   }
}